<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Mapsui.Samples.Maui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:Mapsui.Samples.Maui.View"
    Shell.FlyoutBehavior="Disabled">

    <TabBar x:Name="SKGLViewTabs">
        <Tab Title="Map">
            <ShellContent ContentTemplate="{DataTemplate view:MainPage}" />
        </Tab>
        <Tab Title="Tab2">
            <ShellContent ContentTemplate="{DataTemplate view:EmptyPage}" />
        </Tab>
        <Tab Title="Tab3">
            <ShellContent ContentTemplate="{DataTemplate view:EmptyPage}" />
        </Tab>
    </TabBar>

</Shell>
